#include<iostream>
using namespace std;


int main ()
{

    string name;
    int age;

    cout<<"Enter Your age :";
    cin>>age;
    cout<<"Enter Your name :";
    cin>>name;

   if(age%2==0)
   {
       for(int x=0;x<10;x++)
       {
           cout<<name<<endl;
       }
   }
   else
   {
       for (int x=0 ;x<5;x++)
       {
           cout<<name<<endl;
       }
   }
   return 0;
}
