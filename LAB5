//CAP to accept a number and determine if it is a prime number or composite number
//written by KimluxDuch
#include <iostream>
using namespace std;
int main()
{
    int i,n,x=0;
   cout<<"Enter a number"<<endl;
    cin>>n;
    for (i=1;i<=n;i++)
    {
        if(n%i==0)
        x=x+1;
    }
    if (x==2)
    cout<<"The number is a Prime number"<<endl;
    else
    cout<<"The number is a Composite number"<<endl;
    return 0;
}
